{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "@odata.context": {
      "type": "string"
    },
    "@odata.nextLink": {
      "type": "string"
    },
    "value": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "Id": {
            "type": "integer"
          },
          "CountryId": {
            "type": "string"
          },
          "Name": {
            "type": "string"
          },
          "Population": {
            "type": "integer"
          },
          "Density": {
            "type": "number"
          },
          "AverageTemperatureMin": {
            "type": "number"
          },
          "AverageTemperatureMax": {
            "type": "number"
          },
          "Date": {
            "type": "string",
            "format": "date-time"
          },
          "Tags": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "Region": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "Id": {
                "type": "string"
              },
              "Name": {
                "type": "string"
              }
            },
            "required": [
              "Id",
              "Name"
            ]
          }
        },
        "required": [
          "Id",
          "CountryId",
          "Name",
          "Population",
          "Density",
          "Tags",
          "AverageTemperatureMin",
          "AverageTemperatureMax",
          "Date",
          "Region"
        ]
      }
    }
  },
  "required": [
    "@odata.context",
    "value"
  ]
}
